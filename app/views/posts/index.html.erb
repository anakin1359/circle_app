<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<table>
  <thead>
    <tr>
      <th>件名</th>
      <th>投稿内容</th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |f| %>
      <tr>
        <td><%= f.subject %></td>
        <td><%= f.comment %></td>
      <tr>
    <% end %>
  </tbody>
</table>

